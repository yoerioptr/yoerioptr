{% set classes = [
  theme_hook_original,
  theme_hook_original ~ '__' ~ entity_type,
  theme_hook_original ~ '__' ~ field_name,
  theme_hook_original ~ '__' ~ entity_type ~ '--' ~ field_name,
] %}

<{{ outer_wrapper ?: 'div' }} {{ attributes.addClass(classes) }}>
  {% for item in items %}
    <{{ inner_wrapper ?: 'div' }} {{ item.attributes }}>
      {{ item.content }}
    </{{ inner_wrapper ?: 'div' }}>
  {% endfor %}
</{{ outer_wrapper ?: 'div' }}>
